<form method="post" accept-charset="utf-8">
	<fieldset>
		<legend>Registration</legend>
		<label for="username">Username: </label>
		<input type="text" name="username" id="username" value="" placeholder="username" class="text ui-widget-content ui-corner-all" />
		<label for="password">Password: </label>
		<input type="password" name="password" id="password" value="" placeholder="password" class="text ui-widget-content ui-corner-all" />
		<label for="repeat-password">Repeat Password: </label>
		<input type="password" name="repeat-password" id="repeat-password" value="" placeholder="password" class="text ui-widget-content ui-corner-all" />
		<input type="submit" name="submit" id="submit" value="OK" class="btn btn-primary" />
	</fieldset>	
</form>